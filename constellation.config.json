{
    "files": {
      "package.json": {
        "type": "json",
        "variables": ["projectName", "projectDescription", "author"]
      },
      "README.md": {
        "type": "markdown",
        "variables": ["projectName", "projectDescription", "author"]
      },
      "src/app.module.ts": {
        "type": "typescript",
        "variables": ["projectName"]
      },
      "docker-compose.yml": {
        "type": "yaml",
        "variables": ["projectName"]
      },
      "terraform/main.tf": {
        "type": "terraform",
        "variables": ["projectName"]
      },
      "terraform/variables.tf": {
        "type": "terraform",
        "variables": ["projectName"]
      },
      "terraform/bootstrap/outputs.tf": {
        "type": "terraform",
        "variables": ["projectName"]
      },
      "terraform/bootstrap/variables.tf": {
        "type": "terraform",
        "variables": ["projectName"]
      }
    },
    "ignore": [
      "scripts",
      "template.engine.json",
      "node_modules",
      ".next",
      ".git",
      ".swc",
      ".cursor",
      "dist",
      "build",
      "coverage"
    ]
  } 